<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.yusong.yhdg.batteryserver.persistence.basic.PushMetaDataMapper">

    <insert id="insert" parameterType="cn.com.yusong.yhdg.common.domain.basic.PushMetaData" useGeneratedKeys="true" keyProperty="id">
        insert into bas_push_meta_data(source_type, source_id, create_time)
        values(#{sourceType}, #{sourceId}, #{createTime})
    </insert>

</mapper>