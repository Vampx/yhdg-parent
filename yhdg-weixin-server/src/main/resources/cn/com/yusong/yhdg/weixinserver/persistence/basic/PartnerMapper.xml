<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.yusong.yhdg.weixinserver.persistence.basic.PartnerMapper">

    <select id="find" resultType="cn.com.yusong.yhdg.common.domain.basic.Partner">
        select
          *
        from bas_partner
        where id = #{id}
    </select>

    <select id="findAll" resultType="cn.com.yusong.yhdg.common.domain.basic.Partner">
        select
          *
        from bas_partner
    </select>

</mapper>