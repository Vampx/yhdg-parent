<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.yusong.yhdg.agentappserver.persistence.basic.ShopRolePermMapper">

    <select id="findShopRoleAll" resultType="string">
        select
        perm_id as permId
        from bas_shop_role_perm
        where role_id = #{shopRoleId}
    </select>

</mapper>