<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.yusong.yhdg.serviceserver.persistence.basic.UserNoticeMessageMapper">
     <insert id="insert" parameterType="cn.com.yusong.yhdg.common.domain.basic.UserNoticeMessage">
         insert into bas_user_notice_message
         (type,title,content,user_id,user_mobile,user_fullname,create_time)
         VALUES
         (#{type},#{title},#{content},#{userId},#{userMobile},#{userFullname},#{createTime})
     </insert>
</mapper>