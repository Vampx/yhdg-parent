<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.yusong.yhdg.appserver.persistence.zd.RentInstallmentSettingMapper">

    <select id="find" resultType="cn.com.yusong.yhdg.common.domain.zd.RentInstallmentSetting">
        select
        *
        from zd_rent_installment_setting
        where id = #{id}
    </select>

    <select id="findByMobile" resultType="cn.com.yusong.yhdg.common.domain.zd.RentInstallmentSetting">
        select
         *
        from zd_rent_installment_setting
        where mobile = #{mobile}
    </select>

</mapper>