<div class="popup_body" style="padding-left:50px;font-size: 14px;min-height: 83%;">
    <div class="ui_table">
        <form method="post">
            <input type="hidden" name="id" value="${(entity.id)!''}">
            <table cellpadding="0" cellspacing="0" >
                <tr>
                    <td width="90" align="left">运营商：</td>
                    <td>
                        <input name="agentId" id="agent_id_${pid}" class="easyui-combotree"
                               editable="false" style="width: 185px; height: 28px;" value="${(entity.agentId)!''}" disabled
                               data-options="url:'${contextPath}/security/basic/agent/tree.htm',
                                    method:'get',
                                    valueField:'id',
                                    textField:'text',
                                    editable:false,
                                    multiple:false,
                                    panelHeight:'200'
                                "
                        >
                    </td>
                </tr>
                <tr>
                    <td align="left" width="120px;">车辆型号名称：</td>
                    <td><input type="text" class="text easyui-validatebox" name="modelName" maxlength="40" readonly
                               value="${(entity.modelName)!''}"/></td>
                </tr>
                <tr>
                    <td width="70" align="left">备注：</td>
                    <td colspan="3"><textarea style="width:260px;height:50px;" readonly name="memo" maxlength="200">${(entity.memo)!''}</textarea></td>
                </tr>
                <tr>
                    <td align="left">是否启用：</td>
                    <td>
                        <span class="radio_box">
                            <input type="radio" class="radio" name="isActive" id="is_active_1" <#if entity.isActive?? && entity.isActive == 1>checked</#if>
                                   value="1"/><label for="is_active_1">启用</label>
                        </span>
                        <span class="radio_box">
                            <input type="radio" class="radio" name="isActive" id="is_active_0" <#if entity.isActive?? && entity.isActive == 0>checked</#if>
                                   value="0"/><label for="is_active_0">禁用</label>
                        </span>
                    </td>
                </tr>
                <tr>
                    <td align="left">车型外观图：</td>
                    <td>
                        <div style="width: 350px;">
                            <div id="model_image_div_${pid}" style="float:left;padding-left: 10px;<#if entity.modelImagePath?? && entity.modelImagePath?length gt 0>display:block;<#else>display:none;</#if>">
                                <div class="model_image">
                                    <a href="javascript:void(0)" id="show_model_image_range">
                                        <div style="height: 70%;">
                                            <img id="model_image_${pid}"  src="<#if entity.modelImagePath?? && entity.modelImagePath?length gt 0>${(controller.appConfig.staticUrl)!''}${entity.modelImagePath}</#if>" style="width: 100px;height: 100px;"/>
                                            <p id="show_max_model_image_${pid}" class="model_max_image" style="float: left;">放大</p>
                                            <p id="show_min_model_image_${pid}" class="model_min_image" style="display: none;">缩小</p>
                                        </div>
                                    </a>
                                    <input  type="hidden" id="model_image_path_${pid}" name="modelImagePath" value="${(entity.modelImagePath)!''}"/>
                                </div>
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td align="left">详情页浏览图：</td>
                    <td>
                        <div style="width: 700px;">
